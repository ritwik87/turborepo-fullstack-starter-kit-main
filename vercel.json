{
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "/apps/api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/apps/api/src/main.ts",
      "methods": ["GET"]
    }
  ]
}
